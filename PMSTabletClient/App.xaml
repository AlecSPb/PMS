<Application x:Class="PMSTabletClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PMSTabletClient"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" xmlns:vm="clr-namespace:PMSTabletClient.ViewModel" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/CommonBrushes.xaml"/>
                <ResourceDictionary Source="Resource/Language/zh-cn.xaml"/>
                <ResourceDictionary Source="Resource/Skins/Warm.xaml"/>
                <ResourceDictionary Source="Resource/Styles/Tablet.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

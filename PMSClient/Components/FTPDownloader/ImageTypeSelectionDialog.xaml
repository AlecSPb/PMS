<Window x:Class="PMSClient.Components.FTPDownloader.ImageTypeSelectionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PMSClient.Components.FTPDownloader"
        mc:Ignorable="d"
        WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen"
        Title="图片类型选择" Height="100" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Name="BtnBonding" Click="BtnBonding_Click" 
                Grid.Column="0" Style="{StaticResource StyleButtonNavigation}" Content="Bonding"/>
        <Button Name="BtnTarget" Click="BtnTarget_Click"
                Grid.Column="1" Style="{StaticResource StyleButtonNavigation}" Content="Target"/>
    </Grid>
</Window>

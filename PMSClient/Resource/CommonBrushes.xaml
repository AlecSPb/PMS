<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="CompletedBrush" Color="White"/>
    <SolidColorBrush x:Key="VHPCompletedBrush" Color="Wheat"/>
    <SolidColorBrush x:Key="UnCompletedBrush" Color="#00FF80"/>
    <SolidColorBrush x:Key="UnknownBrush" Color="#04A8DD"/>
    <SolidColorBrush x:Key="WastedBrush" Color="#0D4A4A"/>

    <SolidColorBrush x:Key="PausedBrush" Color="LightGray"/>
    <SolidColorBrush x:Key="UnCheckedBrush" Color="Yellow"/>
    <SolidColorBrush x:Key="EmergencyBrush" Color="LightCoral"/>
    <SolidColorBrush x:Key="CheckedBrush" Color="White"/>
    <SolidColorBrush x:Key="FailedBrush" Color="Gray"/>
    <SolidColorBrush x:Key="NoInputBrush" Color="LightSalmon"/>
    
    <SolidColorBrush x:Key="YesterdayBrush" Color="LightBlue"/>
    <SolidColorBrush x:Key="TodayBrush" Color="LightGreen"/>
    <SolidColorBrush x:Key="TomorrowBrush" Color="LightSalmon"/>

    <SolidColorBrush x:Key="StoredBrush" Color="LightGreen"/>
    <SolidColorBrush x:Key="DeliveredBrush" Color="White"/>

    <SolidColorBrush x:Key="ReadOnlyTextBoxBrush" Color="LightSteelBlue"/>
    
</ResourceDictionary>
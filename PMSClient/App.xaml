<Application x:Class="PMSClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:PMSClient.ViewModel" 
             xmlns:local="clr-namespace:PMSClient"
             StartupUri="MainDesktop.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/CommonBrushes.xaml" />
                <ResourceDictionary Source="Resource/Language/zh-cn.xaml" />
                <ResourceDictionary Source="Resource/Skins/Warm.xaml" />
                <ResourceDictionary Source="Resource/Styles/Desktop.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <vm:ViewModelLocator x:Key="Locator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>

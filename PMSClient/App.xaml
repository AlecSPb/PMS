<Application x:Class="PMSClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PMSClient"
             StartupUri="MainDesktop.xaml"
             xmlns:vm="clr-namespace:PMSClient.ViewModel" ShutdownMode="OnMainWindowClose" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="VMLocator.xaml"/>
                <ResourceDictionary Source="Resource/CommonBrushes.xaml" />
                <ResourceDictionary  Source="Language/zh-cn.xaml" />
                <ResourceDictionary Source="Resource/Styles/XSStyle.xaml"/>
                <ResourceDictionary Source="Helpers/Converter/PMSConverter.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

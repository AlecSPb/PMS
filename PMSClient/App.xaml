<Application x:Class="PMSClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainDesktop.xaml"
             xmlns:local="clr-namespace:PMSClient">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" xmlns:vm="clr-namespace:PMSClient.ViewModel" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/CommonBrushes.xaml" />
                <ResourceDictionary Source="Resource/Language/zh-cn.xaml" />
                <ResourceDictionary Source="Resource/Skins/Warm.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

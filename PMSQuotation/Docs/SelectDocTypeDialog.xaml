<Window x:Class="PMSQuotation.Docs.SelectDocTypeDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PMSQuotation.Docs"
        mc:Ignorable="d"
        Title="SelectDocTypeDialog" Height="250" Width="300"
        WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <Button Content="Chinese" Style="{StaticResource StyleButtonStrench}" Name="BtnChinese"
                    Click="BtnChinese_Click"/>
            <Button Content="English" Style="{StaticResource StyleButtonStrench}" Name="BtnEnglish"
                    Click="BtnEnglish_Click"/>
        </StackPanel>
    </Grid>
</Window>

        <endpoint binding="basicHttpBinding" bindingConfiguration="basic"
          name="materialorder" contract="PMSWCFService.ServiceContracts.IMaterialOrderService" />
        <endpoint binding="basicHttpBinding" bindingConfiguration="basic"
          name="misson" contract="PMSWCFService.ServiceContracts.IMissonService" />
        <endpoint binding="basicHttpBinding" bindingConfiguration="basic"
          name="recordtestresult" contract="PMSWCFService.ServiceContracts.IRecordTestResultService" />
        <endpoint binding="basicHttpBinding" bindingConfiguration="basic"
          name="missonwithplan" contract="PMSWCFService.ServiceContracts.IMissonWithPlanService" />

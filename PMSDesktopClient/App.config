<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="MyOrderService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8002/PMSService.svc" binding="basicHttpBinding"
                bindingConfiguration="MyOrderService" contract="ServiceReference.IOrderService"
                name="MyOrderService" />
        </client>
    </system.serviceModel>
</configuration>

<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="customer" />
                <binding name="deliveryAddress" />
                <binding name="vhpdevice" />
                <binding name="vhpmold" />
                <binding name="vhpprocess" />
                <binding name="compound" />
                <binding name="supplier" />
                <binding name="element" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="customer"
                contract="BasicDataService.ICustomerService" name="customer" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="deliveryAddress"
                contract="BasicDataService.IDeliveryAddressService" name="deliveryAddress" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="vhpdevice"
                contract="BasicDataService.IVHPDeviceService" name="vhpdevice" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="vhpmold"
                contract="BasicDataService.IVHPMoldService" name="vhpmold" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="vhpprocess"
                contract="BasicDataService.IVHPProcessService" name="vhpprocess" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="compound"
                contract="BasicDataService.ICompoundService" name="compound" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="supplier"
                contract="BasicDataService.ISupplierService" name="supplier" />
            <endpoint address="http://localhost:63549/BasicDataService.svc"
                binding="basicHttpBinding" bindingConfiguration="element"
                contract="BasicDataService.IElementService" name="element" />
        </client>
    </system.serviceModel>
</configuration>